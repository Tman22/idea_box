<div class="container center">
  <h1>IdeaBox:</h1>
  <span>Search:</span>
  <input type="text" id="search">
  <br>
  <input type="button" value="Sort" id="sort" class="btn btn-default btn-xs">
  <br><br>
  <div class="post border">
    <span>Title:</span>
    <input type="text" id="idea-title">

    <span>Body:</span>
    <input type="text" id="idea-body">

    <span>Quality:</span>
    <select id='idea-quality'>
      <option value="swill">Swill</option>
      <option value="genius">Genius</option>
      <option value="plausible">Plausible</option>
    </select>
    <br><br>
    <input type="button" value="submit" id="create" class="btn btn-default btn-xs">
  </div>
  <br><br>

  <div class="ideas">
  </div>

</div>
